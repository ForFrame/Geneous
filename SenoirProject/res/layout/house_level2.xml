<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home_bg"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/textUser"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_x="633dp"
        android:layout_y="30dp"
        android:gravity="right|center_vertical"
        android:text="Username"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="16pt"
        android:textStyle="bold"
        android:typeface="sans" />

    <Button
        android:id="@+id/loginn"
        android:layout_width="127dp"
        android:layout_height="wrap_content"
        android:layout_x="849dp"
        android:layout_y="32dp"
        android:background="@drawable/login_txt" />

    <Button
        android:id="@+id/logout"
        android:layout_width="127dp"
        android:layout_height="wrap_content"
        android:layout_x="849dp"
        android:layout_y="32dp"
        android:background="@drawable/logout_txt" />
    
    <ImageView
        android:id="@+id/boxImg"
        android:layout_width="320dp"
        android:layout_height="220dp"
        android:layout_x="400dp"
        android:layout_y="480dp"
        android:background="@drawable/box" />

    <Button
        android:id="@+id/longFrame"
        android:layout_width="520dp"
        android:layout_height="180dp"
        android:layout_x="50dp"
        android:layout_y="125dp"
        android:background="@drawable/frame_long1" />

     <ImageView
         android:id="@+id/longFrameImg"
         android:layout_width="520dp"
         android:layout_height="180dp"
         android:layout_x="50dp"
         android:layout_y="125dp"
         android:background="@drawable/frame_long" />

    <ImageView
        android:id="@+id/circleFrameImg"
        android:layout_width="200dp"
        android:layout_height="250dp"
        android:layout_x="750dp"
        android:layout_y="75dp"
        android:background="@drawable/frame_circle" />
    
    <Button
        android:id="@+id/backToselectHouse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="898dp"
        android:layout_y="653dp"
        android:background="@drawable/icon_home" />

    <Button
        android:id="@+id/showGameHighScore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="653dp"
        android:background="@drawable/icon_highscore" />

   

    

</AbsoluteLayout>
